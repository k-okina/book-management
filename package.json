{
  "name": "book-management",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
    "screenshot": "zisui --serverCmd \"yarn serve:storybook\" http://localhost:6006",
    "zisui": "zisui http://localhost:6006",
    "regression": "reg-suit run",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/firebase": "^3.2.1",
    "@types/highlight.js": "^9.12.3",
    "@types/marked": "^0.4.2",
    "element-ui": "^2.4.8",
    "firebase": "^5.6.0",
    "firebase-admin": "^6.3.0",
    "highlight.js": "^9.13.1",
    "inversify": "^5.0.1",
    "marked": "^0.5.1",
    "pring": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vue-router-layout": "^0.1.2",
    "vuetify": "^1.3.12",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@google-cloud/firestore": "^0.19.0",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@types/jest": "^23.1.4",
    "@types/node": "^10.9.2",
    "@vue/babel-preset-app": "^3.1.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "dotenv-webpack": "^1.5.7",
    "flush-promises": "^1.0.2",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "reg-keygen-git-hash-plugin": "^0.7.6",
    "reg-notify-github-plugin": "^0.7.6",
    "reg-publish-gcs-plugin": "^0.7.6",
    "reg-suit": "^0.7.15",
    "sass-loader": "^7.0.1",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.3",
    "vue-auto-routing": "^0.3.0",
    "vue-cli-plugin-auto-routing": "^0.2.1",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-template-compiler": "^2.5.17",
    "zisui": "^1.2.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "src/**/*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "src/*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "src/**/*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
